<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.nyobaksal"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="25" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.asus.nyobaksal.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.asus.nyobaksal.buttonbuttom"
            android:label="@string/title_activity_buttonbuttom" />
        <activity android:name="com.example.asus.nyobaksal.SearchActivity" />
        <activity android:name="com.example.asus.nyobaksal.nwArrv" />
        <activity android:name="com.example.asus.nyobaksal.SignUp" />
        <activity
            android:name="com.example.asus.nyobaksal.LoginActivity2"
            android:label="@string/title_activity_login2" />
        <activity android:name="com.example.asus.nyobaksal.ShoppingCart" />
        <activity android:name="com.example.asus.nyobaksal.SearchBrands" />
        <activity android:name="com.example.asus.nyobaksal.SearchPencilAndPen" />
        <activity android:name="com.example.asus.nyobaksal.SearchNotebook" />
        <activity android:name="com.example.asus.nyobaksal.SearchPencilCase" />
        <activity android:name="com.example.asus.nyobaksal.SearchEraser" />
        <activity android:name="com.example.asus.nyobaksal.SearchStickyNote" />
    </application>

</manifest>